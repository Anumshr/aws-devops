pipeline {
	agent any
	stages {
		stage('Check lint'){
			steps{
				sh 'tidy -q -e *.html'

			}
	    stage ('upload to s3 bucket'){
	    	steps{
	    		withAWS(region: 'us=west-2', credentials: 'jenkinsaccesskey'){
	    		s3Upload(bucket: 'static-jenkins-pipeline-anukriti1', pathStyleAccessEnabled: false, includePathPattern: 's3bucketupload/index.html'}
	    		)
	    	}
	    }
		}
	}
}
